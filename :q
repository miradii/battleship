import { playerGameBoard, comGameBoard } from "../../src/gameobjects/Game";

test("if player gamboard has five different ships on it", () => {
  expect(playerGameBoard.shipsOnTheBoard.size).toBe(5);
});

test("if computer gamboard has five different ships on it", () => {
  expect(comGameBoard.shipsOnTheBoard.size).toBe(5);
});
